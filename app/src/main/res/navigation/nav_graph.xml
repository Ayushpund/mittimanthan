<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph">

    <fragment
        android:id="@+id/dashboardFragment"
        android:name="com.example.pict.Dashboard"
        android:label="Dashboard">
        
        <action
            android:id="@+id/action_dashboard_to_quickReport"
            app:destination="@id/quickReport" />
            
        <action
            android:id="@+id/action_dashboard_to_chatbot"
            app:destination="@id/chatbot" />
    </fragment>

    <fragment
        android:id="@+id/quickReport"
        android:name="com.example.pict.QuickReport"
        android:label="Quick Report" />

    <fragment
        android:id="@+id/chatbot"
        android:name="com.example.pict.Chatbot"
        android:label="Chatbot" />
    
</navigation>